                <geometry>
                    <cylinder length="0.220" radius="0.01"/>
                </geometry>
            <material name="white"/>
        </visual>

        <collision>
        <geometry>
            <cylinder length="0.220" radius="0.01"/>
        </geometry>
        </collision>

        <inertial>
        <mass value="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
        </inertial>

    </link>

    <joint name="back_right_rod_joint" type="continuous">
        <origin rpy="0 0 0" xyz="-0.090 -0.090 0.0775" />
        <parent link="chassis" />
