        <parent link="back_roll_link" />
        <child link="back_pitch_link" />
        <axis xyz="0 1 0" />
        <limit effort="1000.0" velocity="100.0" />
        <dynamics damping="0.0" friction="0.1"/>
    </joint>


    <!--Front Right Rod-->
    <link name="front_right_rod">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.110"/>
                <geometry>
                    <cylinder length="0.220" radius="0.01"/>
                </geometry>
            <material name="white"/>
        </visual>

        <collision>
        <geometry>
            <cylinder length="0.220" radius="0.01"/>
        </geometry>
        </collision>

        <inertial>
        <mass value="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
        </inertial>

    </link>

    <joint name="front_right_rod_joint" type="continuous">
