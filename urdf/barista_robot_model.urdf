<?xml version="1.0"?>
<robot name="barista_robot">     
    
    <material name="red">
      <color rgba="1.0 0.0 0.0 1"/>
    </material>

    <material name="white">
      <color rgba="1.0 1.0 1.0 1"/>
    </material>

    <material name="black">
      <color rgba="0.0 0.0 0.0 1"/>
    </material>

    <material name="green_light">
        <color rgba="0.0 1.0 0.0 1"/>
    </material>

    <material name="green_dark">
        <color rgba="0.0 0.5 0.0 1"/>
    </material>

    <material name="blue">
        <color rgba="0.0 0.0 1.0 1"/>
    </material>
    
    <link name="base_link"></link>

    <!-- Body -->
    <link name="chassis">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <cylinder length="0.155" radius="0.178"/>
                </geometry>
            <material name="blue"/>
        </visual>

        <collision>
        <geometry>
            <cylinder length="0.155" radius="0.178"/>
        </geometry>
        </collision>

        <inertial>
        <mass value="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
        </inertial>

    </link>

    <joint name="base_link_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.1127" />
        <parent link="base_link" />
        <child link="chassis" />
    </joint>

    <!--Right Motor Wheel-->
    <link name="right_motor_wheel">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
                <geometry>
                    <cylinder length="0.0206" radius="0.0352"/>
                </geometry>
            <material name="black"/>
        </visual>

        <collision>
        <geometry>
            <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        </collision>

        <inertial>
        <mass value="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
        </inertial>

    </link>

    <joint name="right_motor_wheel_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.158 -0.0775" />
        <parent link="chassis" />
        <child link="right_motor_wheel" />
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>


    <!--Left Motor Wheel-->
    <link name="left_motor_wheel">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
                <geometry>
                    <cylinder length="0.0206" radius="0.0352"/>
                </geometry>
            <material name="black"/>
        </visual>

        <collision>
        <geometry>
            <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        </collision>

        <inertial>
        <mass value="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
        </inertial>

    </link>

    <joint name="left_motor_wheel_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.158 -0.0775" />
        <parent link="chassis" />
        <child link="left_motor_wheel" />
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <!-- Caster Wheel Front -->
    <link name="front_yaw_link">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
            <cylinder length="0.05" radius="0.0152"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <joint name="front_yaw_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0.128 0 -0.075" />
        <parent link="chassis" />
        <child link="front_yaw_link" />
        <axis xyz="0 0 1" />
        <limit effort="1000.0" velocity="100.0" />
        <dynamics damping="0.0" friction="0.1"/>
    </joint>



    <link name="front_roll_link">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
            <cylinder length="0.05" radius="0.0152"/>
            </geometry>
            <material name="red"/>
        </visual>
    </link>

    <joint name="front_roll_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="front_yaw_link" />
        <child link="front_roll_link" />
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="100.0" />
        <dynamics damping="0.0" friction="0.1"/>
    </joint>



    <link name="front_pitch_link">
        <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
            <sphere radius="0.0352"/>
        </geometry>
        <material name="green_dark"/>
        </visual>
    </link>

    <joint name="front_pitch_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="front_roll_link" />
        <child link="front_pitch_link" />
        <axis xyz="0 1 0" />
        <limit effort="1000.0" velocity="100.0" />
        <dynamics damping="0.0" friction="0.1"/>
    </joint>

    <!-- Caster Wheel Back -->
    <link name="back_yaw_link">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
            <cylinder length="0.05" radius="0.0152"/>
